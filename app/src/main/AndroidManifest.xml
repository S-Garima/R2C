<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".base.JswApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:hardwareAccelerated="true"
        android:supportsRtl="true"
        android:theme="@style/Theme.R2C"
        tools:targetApi="31">
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="userPortrait"
            tools:replace="screenOrientation" />
        <activity
            android:name=".presentation.activities.DashboardActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
        <activity
            android:name=".presentation.activities.productionhead.DashboardProductionHeadActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
        <activity
            android:name=".presentation.activities.manager.DashboardManagerApprovalactivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />

        <activity
            android:name=".presentation.activities.storeincharge.DashboardStoreInchargeActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
        <activity
            android:name=".presentation.activities.packagingsupervisor.DashboardPackagingSupervisorActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
         <activity
            android:name=".presentation.activities.gatekeeper.DashboardGateKeeperActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
        <activity
            android:name=".presentation.activities.gatekeeper.GatePassApprovalScreen"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />
         <activity
            android:name=".presentation.activities.transportperson.DashboardTransportPersonActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />

         <activity
            android:name=".presentation.activities.productionsupervisor.DashboardProductionSupervisorActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.R2C" />



        <activity
            android:name=".presentation.activities.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.R2C">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>